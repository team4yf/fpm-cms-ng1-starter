{
  "name": "fpm-cms-ng1-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "./node_modules/.bin/nodemon source/app.js",
    "test": "./node_modules/.bin/mocha test/*.test.js",
    "demo": "./node_modules/.bin/http-server ./node_modules/admin-lte/ -d False -s --cors -c-1 -p 8088",
    "proxy": "ngrok http 9007",
    "docker:clean": "docker stop $(docker ps -qa) && docker rm $(docker ps -aq)",
    "bot": "node fake/bot.js",
    "backup": "./backup.sh",
    "docker": "docker-compose -f docker-compose.dev.yml up --build -d",
    "docker:restart": "docker-compose -f docker-compose.dev.yml down && npm run docker",
    "run:pm2": "pm2 start source/app.js --name cms-app -i 1",
    "build": "./node_modules/.bin/gulp"
  },
  "devDependencies": {
    "admin-lte": "^2.4.8",
    "echarts": "^4.2.0-rc.2",
    "eonasdan-bootstrap-datetimepicker": "^4.17.47",
    "gulp": "^4.0.0",
    "gulp-jshint": "^2.0.4",
    "http-server": "^0.11.1",
    "jshint": "^2.9.4",
    "merge-stream": "^1.0.1",
    "mocha": "^3.2.0",
    "moment": "^2.23.0",
    "nodemon": "^1.18.4",
    "sweetalert2": "^7.28.11"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.2",
    "debug": "^4.1.1",
    "fpm-plugin-mysql": "^1.3.0",
    "fpm-plugin-rbac-mysql": "^1.0.2",
    "fpm-plugin-socketio": "^1.0.4",
    "fpm-plugin-upload": "0.0.4",
    "fpm-plugin-user-service": "^1.0.2",
    "fpmc-jssdk": "^1.0.7",
    "koa-session": "^5.9.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.1.4",
    "nunjucks": "^3.1.3",
    "uuid": "^3.3.2",
    "yamljs": "^0.3.0",
    "yf-fpm-server": "^2.5.4"
  }
}
