{
  "name": "fpm-cms-ng1-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "./node_modules/.bin/nodemon source/app.js",
    "test": "./node_modules/.bin/mocha test/*.test.js",
    "demo": "./node_modules/.bin/http-server ./node_modules/admin-lte/ -d False -s --cors -c-1 -p 8088",
    "proxy": "ngrok http 9007",
    "bot": "node fake/bot.js",
    "backup": "python backup.py",
    "docker": "docker-compose -f docker/docker-compose.dev.yml up --build -d",
    "docker:shutdown": "docker-compose -f docker/docker-compose.dev.yml down",
    "docker:restart": "docker-compose -f docker/docker-compose.dev.yml down && npm run docker",
    "run:pm2": "pm2 start source/app.js --name [yourname] -i 1 -o logs/log.log",
    "build": "gulp"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "gulp": "^4.0.0",
    "gulp-jshint": "^2.0.4",
    "http-server": "^0.11.1",
    "jshint": "^2.9.4",
    "merge-stream": "^1.0.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.18.4",
    "yf-fpm-client-js": "^1.0.2",
    "echarts": "^4.2.0-rc.2",
    "admin-lte": "^2.4.8",
    "sweetalert2": "^7.28.11",
    "yfpmc-ng1": "^1.0.4"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.2",
    "fpm-plugin-mysql": "^1.1.1",
    "fpm-plugin-socketio": "^1.0.4",
    "fpm-plugin-upload": "0.0.4",
    "koa-multer": "^1.0.2",
    "koa-session": "^5.9.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.1.4",
    "nunjucks": "^3.1.3",
    "uuid": "^3.3.2",
    "yamljs": "^0.3.0",
    "yf-fpm-dbm": "^2.0.7",
    "yf-fpm-server": "^2.4.12"
  }
}
